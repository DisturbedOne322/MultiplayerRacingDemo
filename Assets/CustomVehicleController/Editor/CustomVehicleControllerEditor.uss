    -unity-font: url('project://database/Assets/CustomVehicleController/Editor/Font/Vegur-Yg1a.otf?fileID=12800000&guid=6c91916d973bdd74fa85ea681d8ea7e9&type=3#Vegur-Yg1a');
}

.background {
    flex-grow: 1;
    align-items: flex-start;
    background-color: rgb(50, 55, 55);
    -unity-background-image-tint-color: rgb(0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    -unity-font: url('project://database/Assets/CustomVehicleController/Editor/Font/Vegur-Yg1a.otf?fileID=12800000&guid=6c91916d973bdd74fa85ea681d8ea7e9&type=3#Vegur-Yg1a');
}

.WheelLeft {
    flex-grow: 1;
    align-items: auto;
    justify-content: flex-start;
    align-self: auto;
    flex-direction: column;
    width: 100px;
    background-color: rgba(255, 255, 255, 0);
    height: 0;
    margin-bottom: 0;
    margin-top: 0;
    font-size: 10px;
    -unity-font: url('project://database/Assets/CustomVehicleController/Editor/Font/Vegur-Yg1a.otf?fileID=12800000&guid=6c91916d973bdd74fa85ea681d8ea7e9&type=3#Vegur-Yg1a');
}

.Header {
    -unity-background-scale-mode: stretch-to-fill;
