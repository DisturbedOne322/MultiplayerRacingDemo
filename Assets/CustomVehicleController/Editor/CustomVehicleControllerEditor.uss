    white-space: nowrap;
    transform-origin: center;
    -unity-background-scale-mode: stretch-to-fill;
    -unity-text-outline-width: 0;
    flex-direction: row;
    overflow: hidden;
    margin-top: 5px;
    opacity: 1;
    min-width: auto;
    -unity-font: url('project://database/Assets/CustomVehicleController/Editor/Font/Vegur-Yg1a.otf?fileID=12800000&guid=6c91916d973bdd74fa85ea681d8ea7e9&type=3#Vegur-Yg1a');
