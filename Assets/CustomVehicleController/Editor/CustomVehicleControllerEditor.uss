.custom-label {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(68, 138, 255);
    -unity-font: url('project://database/Assets/CustomVehicleController/Editor/Font/Vegur-Yg1a.otf?fileID=12800000&guid=6c91916d973bdd74fa85ea681d8ea7e9&type=3#Vegur-Yg1a');
}

:root {
    flex-grow: 1;
    height: 100%;
    width: 100%;
